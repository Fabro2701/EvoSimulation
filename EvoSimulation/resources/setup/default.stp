grammars := action; "grammar.StandardGrammar"; "resources/loads/grammars/obesidad_acts.bnf".

global := a(){
			let mu = 0.0004578;
			let beta = 0.001121;
			let gamma = 0.0003226;
			let p1 = 0.020;
			let p2 = 0.042;
			let p = 1/7;
			let e1 = 0.004;
			let e2 = 0.024;
			let N0 = 0.704;
			let S0 = 0.39;
			let O0 = 0.16;
		  }.
init := init1("simulator.model.entity.individuals.MyIndividual"){
			
			  let p = (static "simulator.RandomSingleton").nextFloat();
			  if(p<=O0){
			  	e.setAttribute("imc","O");
			  }
			  else if(p<=S0+O0){
			  	e.setAttribute("imc","S");
			  }
			  else{
			  	e.setAttribute("imc","N");
			  }
			  e.setAttribute("state","towardsHouse");
		}
		initpasive("simulator.model.entity.PasiveEntity"){
			
			 
	 	}.
updates := update1("simulator.model.entity.individuals.MyIndividual"){
				if(p<=mu){
					e.dispose();
				}
				let imc = e.getAttribute("imc");
				let p = (static "simulator.RandomSingleton").nextDouble();
				if(imc.equals("S")){
					if(p<=gamma){
						e.setAttribute("imc","O");
					}
				}
		   }
		   update2("simulator.model.entity.individuals.MyIndividual"){
			   let imc = e.getAttribute("imc");
				let p = (static "simulator.RandomSingleton").nextDouble();
				let sn = p1*p2*p;
				let os = e1*e2*p;
				if(imc.equals("S")){
					if(p<=sn){
						e.setAttribute("imc","N");
					}
				}
				else if(imc.equals("O")){
					if(p<=os){
						e.setAttribute("imc","S");
					}
				}
		   }
		   move("simulator.model.entity.individuals.MyIndividual"){
					    
			    if(!e.hasAttribute("house")){
			    	let house = e.getTowardsRandomAttribute((e.getCtrl()).getSimulator(),"house");
			    	e.setAttribute("house",house);
			    }
			    if(!e.hasAttribute("supermarket")){
			    	let supermarket = e.getTowardsRandomAttribute((e.getCtrl()).getSimulator(),"supermarket");
			    	e.setAttribute("supermarket",supermarket);
			    }
			    
		   		let state = e.getAttribute("state");
		   		if(state.equals("towardsSupermarket")){
		   			let node = e.nextNodeTowards(simulator.getMap(), e.getAttribute("supermarket"));
					if(node != null)e.setNode(node);
				}
				else if(state.equals("towardsHouse")){
					let node = e.nextNodeTowards(simulator.getMap(), e.getAttribute("house"));
					if(node != null)e.setNode(node);
				}
		   }.
actions := <action> NOTHING{
		   					;
		   				   }.  
interactions := OS_INTERACTION("simulator.model.entity.individuals.MyIndividual"){
					if(!e2.getAttribute("pasive")){
						let imc1 = e1.getAttribute("imc");
						let imc2 = e2.getAttribute("imc");
						let p = (static "simulator.RandomSingleton").nextDouble();
						if(imc1.equals("N")&&((imc2.equals("S")||imc2.equals("O")))){
							if(p<=beta){
								e1.setAttribute("imc","S");
							}
						}
					}
					if(e2.getAttribute("pasive")){
						if(e2.hasAttribute("house")){
							e1.setAttribute("state","towardsSupermarket");
						}
						else if(e2.hasAttribute("supermarket")){
							e1.setAttribute("state","towardsHouse");
						}
					}
				}.